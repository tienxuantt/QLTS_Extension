{
  "manifest_version": 3,
  "name": "QLTS_Extension",
  "description": "Phần mềm tiện ích giúp nhập nhanh vào phần mềm cơ sở dữ liệu quốc gia",
  "version": "1.0",
  "permissions": ["storage","scripting", "activeTab"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}
